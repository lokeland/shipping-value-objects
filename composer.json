{
    "name": "lokeland/shipping-value-objects",
    "description": "Collection of value objects regularly used when working with products and shipping.",
    "keywords": [
        "lokeland",
        "valueobjects",
        "shipping",
        "weight",
        "dimensions"
    ],
    "homepage": "https://github.com/lokeland/shipping-value-objects",
    "license": "MIT",
    "authors": [
        {
            "name": "Jarand",
            "email": "jarand@lokeland.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/collections": ">=9.47"
    },
    "require-dev": {
        "pestphp/pest": "^1.22",
        "laravel/pint": "^1.4",
        "spatie/ray": "^1.36"
    },
    "autoload": {
        "psr-4": {
            "Lokeland\\SVO\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lokeland\\SVO\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
